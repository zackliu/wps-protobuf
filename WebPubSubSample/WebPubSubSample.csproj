<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Messaging.WebPubSub.Client" Version="1.0.0-beta.2" />
	<PackageReference Include="Google.Protobuf" VersionOverride="3.21.9" Version="3.25.1" />
	<PackageReference Include="Grpc.Tools" VersionOverride="2.50.0" Version="2.60.0">
		<PrivateAssets>all</PrivateAssets>
		<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	</PackageReference>
  </ItemGroup>

  <ItemGroup>
	  <Protobuf Include="Protos\webpubsub.client.proto" />
  </ItemGroup>

</Project>
